{
  "name": "rs-docsrs-mcp",
  "version": "0.3.1",
  "description": "Model Context Protocol (MCP) server for docs.rs - Search and fetch Rust crate documentation",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "rs-docsrs-mcp": "src/index.js"
  },
  "files": [
    "src/**/*.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node src/index.js",
    "prepublishOnly": "echo 'Publishing docsrs-mcp...'"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "docs.rs",
    "rust",
    "documentation",
    "crate",
    "ai",
    "llm",
    "claude"
  ],
  "author": "ZhenYi <zheyqaq@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lazhenyi/rs-docsrs-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/lazhenyi/rs-docsrs-mcp/issues"
  },
  "homepage": "https://github.com/lazhenyi/rs-docsrs-mcp",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "cheerio": "^1.0.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.4"
  }
}
